<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <Authors>Diego B. Fernandez</Authors>
    <Company />
    <Product />
    <Description>A package to help parsing and injecting .env file into environment variables.</Description>
    <PackageTags>envvar dotenv twelve-factor-app</PackageTags>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
